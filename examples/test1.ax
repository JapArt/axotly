TEST POST create a resource
  POST https://httpbin.org/post
  Content-Type: application/json

  BODY
  {
    "name": "Axotly",
    "role": "tester"
  }
  BODYEND
  
  EXPECT status == 200
  EXPECT body.json.name == "Axotly"
  EXPECT body.json.role == "admin"
END

